{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "build:css": "tailwind build src/styles/global.css -o src/styles/tailwind.css",
    "build:tailwind": "NODE_ENV=production npx tailwindcss-cli@latest build src/styles/index.css -o ./static/dist/tailwind.css",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.2.0",
    "@auth0/auth0-spa-js": "^1.13.6",
    "@headlessui/react": "^0.2.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.3",
    "@reach/menu-button": "^0.13.0",
    "@reach/router": "^1.3.4",
    "autoprefixer": "^10.2.3",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^6.0.0",
    "gatsby": "^2.26.1",
    "gatsby-image": "^2.10.0",
    "gatsby-plugin-create-client-paths": "^2.9.0",
    "gatsby-plugin-manifest": "^2.11.0",
    "gatsby-plugin-postcss": "^3.6.0",
    "gatsby-plugin-react-helmet": "^3.9.0",
    "gatsby-plugin-sass": "^3.1.0",
    "gatsby-plugin-sharp": "^2.13.1",
    "gatsby-source-filesystem": "^2.10.0",
    "gatsby-theme-material-ui": "^1.0.13",
    "gatsby-transformer-sharp": "^2.11.0",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-md": "^2.5.3",
    "react-redux": "^7.2.2",
    "sass": "^1.32.5",
    "styled-components": "^5.2.1",
    "tailwindcss": "^2.0.2"
  },
  "devDependencies": {
    "node-sass": "^5.0.0",
    "prettier": "2.2.1"
